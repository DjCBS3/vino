#: ../capplet/vino-preferences.c:761
msgid "Send this command by email"
msgstr "Send denne kommandoen via e-post"

#: ../capplet/vino-preferences.c:796
#, c-format
msgid ""
"There was an error displaying help:\n"
"%s"
msgstr ""
"Det oppsto en feil under visning av hjelp:\n"
"%s"

#: ../capplet/vino-preferences.desktop.in.in.h:1
msgid "Remote Desktop"
msgstr "Eksternt skrivebord"

#: ../capplet/vino-preferences.desktop.in.in.h:2
msgid "Set your remote desktop access preferences"
msgstr "Sett brukervalg for tilgang til skrivebord eksternt"

#: ../capplet/vino-preferences.glade.h:1
msgid "<b>Security</b>"
msgstr "<b>Sikkerhet</b>"

#: ../capplet/vino-preferences.glade.h:2
msgid "<b>Sharing</b>"
msgstr "<b>Deling</b>"

#: ../capplet/vino-preferences.glade.h:3
msgid "A_sk you for confirmation"
msgstr "_Spør deg om bekreftelse"

#: ../capplet/vino-preferences.glade.h:4
msgid "Allow other users to _view your desktop"
msgstr "La andre brukere _se ditt skrivebord"

#: ../capplet/vino-preferences.glade.h:5
msgid "Remote Desktop Preferences"
msgstr "Brukervalg for eksternt skrivebord"

#: ../capplet/vino-preferences.glade.h:6
msgid "Some of these preferences are locked down"
msgstr "Noen av disse brukervalgene er låst"

#: ../capplet/vino-preferences.glade.h:7
msgid "Users can view your desktop using this command:"
msgstr "Brukere kan se ditt skrivebord ved å bruke denne kommandoen:"

#: ../capplet/vino-preferences.glade.h:8
msgid "When a user tries to view or control your desktop:"
msgstr "Når en bruker prøver å vise eller kontrollere ditt skrivebord:"

#: ../capplet/vino-preferences.glade.h:9
msgid "_Allow other users to control your desktop"
msgstr "L_a andre brukere kontrollere ditt skrivebord"

#: ../capplet/vino-preferences.glade.h:10
msgid "_Password:"
msgstr "_Passord:"

#: ../capplet/vino-preferences.glade.h:11
msgid "_Require the user to enter this password:"
msgstr "K_rev at bruker oppgir dette passordet:"

#: ../server/vino-fb.c:984 ../server/vino-prompt.c:143
#: ../server/vino-server.c:987
msgid "Screen"
msgstr "Skjerm"

#: ../server/vino-fb.c:985
msgid "The screen to be monitored"
msgstr "Skjerm som skal overvåkes"

#: ../server/vino-main.c:80
msgid ""
"Your XServer does not support the XTest extension - remote desktop access "
"will be view-only\n"
msgstr ""
"Din X-tjener støtter ikke XTest-utvidelsen - fjerntilgang vil kun fungere "
"med visning\n"

#.
